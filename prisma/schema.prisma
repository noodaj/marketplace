// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model Item {
    id       String @id @default(cuid())
    name     String
    price    Int?
    quantity Int
    image    String?
}

model User {
    id       Int    @id 
    username String 
    password String
}
